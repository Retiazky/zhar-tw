{
  "expo": {
    "name": "zhar",
    "slug": "zhar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "com.retiazky.zhar",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#17121c"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.retiazky.zhar",
      "associatedDomains": [],
      "appleTeamId": "U5W3FCHNWS"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/zhar.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.retiazky.zhar",
      "intentFilters": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          },
          "ios": {
            "extraPods": [
              {
                "name": "OpenSSL-Universal",
                "configurations": [
                  "Release",
                  "Debug"
                ],
                "modular_headers": true,
                "version": "3.3.2000"
              }
            ]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    },
    "owner": "retiazky"
  }
}
